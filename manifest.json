{
  "description": "Resolve adobe connect webview in linux system for using in University of Zanjan domain",
  "manifest_version": 2,
  "name": "ZNUlnx-adobeconn-resolver",
  "version": "1.1",
  "homepage_url": "https://github.com/ssharifian/ZNUlnx-adobeconn-resolver",
  "icons": {
    "48": "icons/znu-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.znu.ac.ir/system/get-player?urlPath*"],
      "js": ["redirect.js"]
    }
  ]
}
